<?xml version="1.0" encoding="utf-8" ?>


<!-- These are the default dancers seen when a user opens the game without a code. -->

<default_dancers>
	
	<!-- xPos and yPos are the character's position relative to the center of the stage. -->
	
	<!-- scale is the character's relative proportional scale, with a value of "1" being 100% of original size. -->
	
	<!-- Internal field is the animation id of the dancer. Refer to the animations.xml document -->
	
	<dancer xPos="-330" yPos="-50" scale="1"><![CDATA[3]]></dancer>
	<dancer xPos="335" yPos="-50" scale="1"><![CDATA[1]]></dancer>
	<dancer xPos="-250" yPos="70" scale="1"><![CDATA[2]]></dancer>
	<dancer xPos="250" yPos="70" scale="1"><![CDATA[0]]></dancer>
</default_dancers>
