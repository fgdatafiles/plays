<config>
	
	<!-- CODE UNLOCK -->
	<unlockEnabled>true</unlockEnabled> <!-- Set whether or not to allow the use of the code unlock screen -->
	<unlockCode>00000</unlockCode> <!-- Set the code unlock number here, should be less than 10 digits and you can use the number 0 -->
	
    <!-- SERVICES -->
    <!-- Enable, disable, and configure services here -->
    <!-- Use "wdgwxdgameschopperchase" here. analytics fail gracefully if game is not connected, as long as proper index is in (use the one on clients) -->
    
	<services>

		<service type="analytics" enabled="false" id="wdgwxdgameschopperchase" />

	</services>
	
	<!-- ACHIEVEMENTS -->
	<achievements enabled="true"/>
		
	<!-- LOCALIZATION -->
	      
    <localization>
    	<region>en</region>
    </localization>
    	
    <!-- MINIGAME SETTING -->
    <!--  
    0 - None.
    1 - Swat
    2 - Match 3
    3 - Pong
    -->
    <minigame type="0"/>
    
    <!-- TEST SETTINGS -->
	<minigametest enabled="false"/>
	<videotest enabled="false"/>
	<allmissionsunlocked enabled="false"/>
    	
