<?xml version="1.0" encoding="utf-8" ?>


<!-- Holds the background paths used in game. The default field holds the id value of the default background. -->

<backgrounds defaultid="0"> 
	
	<!-- id field refers to the code presented in the share link and parsed by the game reader. NO LETTERS. 
		Make sure they are all unique within the scope of this document. -->
	
	<!-- inner data is the filename of the background without the file extension. Put the filename inside <![CDATA[ ]]> . -->
	
	<background id="0"><![CDATA[HD-1]]></background>
	<background id="1"><![CDATA[HD-2]]></background>
	<background id="2"><![CDATA[HD-3]]></background>
	<background id="3"><![CDATA[HD-4]]></background>
	<background id="4"><![CDATA[HD-5]]></background>
	<background id="5"><![CDATA[GS-1]]></background>
	<background id="6"><![CDATA[GS-2]]></background>
	<background id="7"><![CDATA[GS-3]]></background>
	<background id="8"><![CDATA[GS-4]]></background>
	<background id="9"><![CDATA[GS-5]]></background>
	
	
	

	
</backgrounds>